{
  "name": "fiap-bytebank-react-native",
  "version": "2.2.1",
  "description": "Template para projetos em React Native, com Estrutura de Pastas customizada, Estilos Globais, ESLint + Prettier + Babel Plugin Root Import, React Navigation, Redux, Redux Saga, Reactotron e Axios configurados.",
  "keywords": [
    "react-native",
    "template",
    "rocketseat",
    "eslint",
    "react-navigation",
    "reactotron",
    "babel-root-import",
    "axios",
    "redux",
    "redux-saga",
    "fiap",
    "andersonlimadeveloper",
    "lemon.dev.br",
    "Anderson Lima"
  ],
  "repository": {
    "type": "https",
    "url": "https://github.com/Andersonlimahw/fiap-bytebank-react-native#"
  },
  "homepage": "https://github.com/Andersonlimahw/fiap-bytebank-react-native#readme",
  "author": "Anderson Lima <andersonlimadeveloper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npx react-native start",
    "start:reset": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "ios:pods": "[ -d ios ] && (cd ios && pod install) || echo 'ios folder not found'",
    "bundle:ios": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "typecheck": "tsc --noEmit",
    "postinstall": "[ -d ios ] && (cd ios && pod install) || true"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.10.1",
    "@tanstack/react-query": "^5.58.3",
    "axios": "^1.7.7",
    "firebase": "^10.12.5",
    "i18next": "^23.14.0",
    "react": "^18.2.0",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^13.5.0",
    "react-native": "^0.74.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.33.0",
    "react-native-svg-transformer": "^1.5.1",
    "styled-components": "^6.1.8",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-typescript": "^7.24.7",
    "@react-native-community/cli": "^20.0.1",
    "@types/react": "^18.2.48",
    "@types/react-native": "^0.73.0",
    "babel-plugin-root-import": "^6.6.0",
    "typescript": "^5.5.4"
  }
}
